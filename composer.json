{
  "name": "bi0r0b0t/silex-pinba-provider",
  "description": "Provide PinbaBundle for Silex",
  "minimum-stability": "stable",
  "version": "1.0.6",
  "license": "MIT",
  "authors": [
    {
      "name": "Mikhail Dolgov",
      "email": "dolgov@bk.ru"
    }
  ],
  "keywords": ["silex", "pinba", "php"],
  "require": {
    "doctrine/dbal": "~2.2",
    "intaro/pinba-bundle": "~0.4.2|^1.0",
    "silex/silex": "~2.0",
    "twig/twig"  : "~1.28|~2.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "gggeek/pinba_php",
        "version": "dev-master",
        "dist": {
          "url": "https://github.com/gggeek/pinba_php/archive/d23c3725a19d53a6f98f91673d73e8f685f6f3d7.zip",
          "type": "zip"
